{
    "compilerOptions": {
      "outDir": "dist/src",
      "module": "esnext",
      "target": "es5",
      "declaration": true,
      "jsx": "react-jsx",
      "moduleResolution":"Node",
      "allowSyntheticDefaultImports": true,
      "skipLibCheck": true,
    },
    "include": [
      "src",
    ],
    "exclude": [
      "src/**/*.test.tsx",
      "src/**/*.stories.tsx",
      "src/setupTests.ts",
    ]
  }