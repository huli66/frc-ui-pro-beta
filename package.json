{
  "name": "frc-ui-pro",
  "version": "2.5.0",
  "author": "sumscope",
  "private": false,
  "main": "dist/src/index.js",
  "module": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "license": "MIT",
  "keywords": [
    "Component",
    "UI",
    "React"
  ],
  "files": [
    "dist"
  ],
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@storybook/addons": "^6.4.13",
    "@storybook/theming": "^6.4.13",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.0.3",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "antd": "~4.20.0",
    "array-move": "^4.0.0",
    "copy-to-clipboard": "^3.3.1",
    "echarts": "^5.3.1",
    "fetch-jsonp": "^1.2.1",
    "immutability-helper": "^3.1.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "node-sass": "^7.0.1",
    "rc-resize-observer": "^1.2.0",
    "react": "^17.0.2",
    "react-dnd": "^15.0.1",
    "react-dnd-html5-backend": "^15.0.1",
    "react-dom": "^17.0.2",
    "react-draggable": "^4.4.4",
    "react-highlight-words": "^0.18.0",
    "react-icons": "^4.3.1",
    "react-markdown": "^8.0.0",
    "react-resizable": "^3.0.4",
    "react-scripts": "5.0.0",
    "react-sortable-hoc": "^2.0.0",
    "react-window": "^1.8.7",
    "typescript": "^4.5.4",
    "web-vitals": "^2.1.2"
  },
  "scripts": {
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "clean": "rimraf ./dist",
    "clean-docs": "rimraf ./storybook-static",
    "build-ts": "tsc -p tsconfig.build.json",
    "build-css": "node-sass ./src/styles/index.scss ./dist/src/styles/global/index.css",
    "cp-public": "cp -R ./public ./dist",
    "build": "npm run clean && npm run build-ts && npm run build-css && npm run cp-public",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "npm run clean-docs && build-storybook -s public",
    "prepublishOnly": "npm run build",
    "start": "npm run storybook",
    "prepare": "husky install"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "prettier"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/addon-actions": "~6.4.13",
    "@storybook/addon-docs": "~6.4.13",
    "@storybook/addon-essentials": "~6.4.13",
    "@storybook/addon-links": "~6.4.13",
    "@storybook/builder-webpack5": "~6.4.13",
    "@storybook/manager-webpack5": "~6.4.13",
    "@storybook/node-logger": "~6.4.13",
    "@storybook/preset-create-react-app": "~4.0.0",
    "@storybook/preset-scss": "~1.0.3",
    "@storybook/react": "~6.4.13",
    "@types/lodash": "^4.14.186",
    "@types/node": "^17.0.9",
    "@types/react-highlight-words": "^0.16.4",
    "@types/react-resizable": "^3.0.2",
    "@types/react-window": "^1.8.5",
    "css-loader": "^6.5.1",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "2.7.1",
    "rimraf": "^3.0.2",
    "sass": "^1.48.0",
    "sass-loader": "^12.4.0",
    "style-loader": "^3.3.1",
    "webpack": "^5.66.0"
  },
  "lint-staged": {
    "*.{js,css,md,ts,tsx}": "prettier --write"
  }
}
